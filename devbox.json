{
  "$schema":  "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": ["nodejs_22@latest"],
  "env": {
    "DEVBOX_COREPACK_ENABLED": "true"
  },
  "shell": {
    "init_hook": [
    ],
    "scripts": {
      "setup": [
        "COREPACK_ENABLE_DOWNLOAD_PROMPT=0 yarn install",
        "yarn run build"
      ],
      "start": [
        "yarn run -s start"
      ]
    }
  }
}
